// M12 Namespace Example: Math utilities module
namespace example.math

// Math utility functions
fn add(a: i64, b: i64) i64 {
    return a + b
}

fn subtract(a: i64, b: i64) i64 {
    return a - b
}

fn multiply(a: i64, b: i64) i64 {
    return a * b
}

test "math functions work" {
    var r1: i64 = add(10, 5)
    if (r1 == 15) {
        print_string("PASS: add")
    }

    var r2: i64 = subtract(10, 3)
    if (r2 == 7) {
        print_string("PASS: subtract")
    }

    var r3: i64 = multiply(4, 5)
    if (r3 == 20) {
        print_string("PASS: multiply")
    }
}
