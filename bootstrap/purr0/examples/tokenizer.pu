// tokenizer.pu - tiny tokenizer skeleton in purr0

struct Token {
  kind: i32;
  pos: i32;
}

fn tokenize(input: string) -> i32 {
  let i: i32 = 0;
  let n: i32 = 0; // runtime provides real length
  while (i < n) {
    let kind: i32 = 0;
    let t: Token = Token{ kind: kind, pos: i };
    if (t.kind == 0) {
      return 1;
    } else {
      let i: i32 = i + 1;
    }
  }
  return 0;
}

fn main() -> i32 {
  return tokenize("x");
}
